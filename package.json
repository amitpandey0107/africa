{
  "name": "universal-demo",
  "version": "6.1.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0",
    "start:local": "ng serve --host 192.168.0.104 --port 4000",
    "start:universal": "npm run build:universal && npm run build:ssr && npm run serve:ssr",
    "build": "ng build --prod --base-href /africa_smb/",
    "build:universal": "ng run universal-demo:server",
    "prod": "ng serve --prod --disable-host-check --port 443 --ssl true --ssl-key zalate.key --ssl-cert zalate.crt",
    "lint": "ng lint universal-demo",
    "build:client-and-server-bundles": "ng build --prod && ng build universal-demo --prod",
    "build:prerender": "npm run build:client-and-server-bundles && npm run compile:server && npm run generate:prerender",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "compile:server": "tsc -p server.tsconfig.json",
    "generate:prerender": "cd dist && node prerender",
    "serve:prerender": "cd dist/browser && http-server",
    "serve:ssr": "node dist/server.js",
    "begin": " ng run universal-demo:server",
    "universal": "npm run build:ssr && npm run serve:ssr",
    "fix:cookie": "node node_modules/babel-cli/bin/babel.js node_modules/ngx-cookie-service/ --out-dir node_modules/ngx-cookie-service/ --presets es2015",
    "fix:filesize": "node node_modules/babel-cli/bin/babel.js node_modules/ngx-filesize/ --out-dir node_modules/ngx-filesize/ --presets es2015",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors"
  },
  "private": true,
